package com.org;

public class Array_Con {
	public static void main(String[] args) {
		int a[] = new int[10];
		int b[] = new int[20];
		a[0] = 1;
		a[1] = 10;
		b[5] = 6;
		b[20] = 20;

		System.out.println(a[0]);
		System.out.println(a[1]);
		System.out.println(b[5]);
		System.out.println(b[20]);

	}
}
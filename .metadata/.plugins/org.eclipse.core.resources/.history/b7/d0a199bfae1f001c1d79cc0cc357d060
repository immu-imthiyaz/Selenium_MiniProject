package string;

public class String_concept {
	public static void main(String[] args) {
		String s = "Hello Riya";
		int length = s.length();
		System.out.println(length);
		
		boolean equals = s.equals(s);
		System.out.println(equals);
		
		boolean equalsIgnoreCase = s.equalsIgnoreCase(s);
		System.out.println(equalsIgnoreCase);
		
		String upperCase = s.toUpperCase();
		System.out.println(upperCase);
		
		String lowerCase = s.toLowerCase();
		System.out.println(lowerCase);
		
		boolean startsWith = s.startsWith("He");
		System.out.println(startsWith);
		
		boolean endsWith = s.endsWith("ya");
		System.out.println(endsWith);
		
		boolean contains = s.contains("lo Ri");
		System.out.println(contains);
		
		int indexOf = s.indexOf('o');
		System.out.println(indexOf);
		
		int lastIndexOf = s.lastIndexOf('o');
		System.out.println(lastIndexOf);
		
		char charAt = s.charAt(4);
		System.out.println(charAt);
		
		String replace = s.replace("l", "lll");
		System.out.println(replace);
		
		
		
		
				
				
				
				
				
				
				
				
				
				
				
				
				
				
				
				
				
				
	}
	

}
